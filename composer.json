{
  "name": "writ3it/cs_modular-monolith",
  "description": "PHPCS Standards for modular monoliths.",
  "type": "phpcodesniffer-standard",
  "license": "MIT",
  "authors": [
    {
      "name": "writ3it.tech",
      "email": "git@writ3it.tech"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "squizlabs/php_codesniffer": "^3.5",
    "ext-simplexml": "*"
  },
  "support": {
    "source": "https://github.com/writ3it/CS_ModularMonolith",
    "issues": "https://github.com/writ3it/CS_ModularMonolith/issues"
  },
  "autoload": {
    "psr-4": {
      "Writ3it\\CodingStandards\\ModularMonolith\\": "ModularMonolith/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Writ3it\\CodingStandards\\Tests\\": "test/"
    },
    "classmap": ["vendor/squizlabs/php_codesniffer/"]
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "ext-json": "*"
  }
}
